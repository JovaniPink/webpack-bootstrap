    },
    "overrides": [],
    "plugins": [],
    "rules": {
      "unicorn/no-abusive-eslint-disable": 0,
      "unicorn/filename-case": [
        "error",
        {
          "cases": {
            "camelCase": true,
            "kebabCase": true
          }
        }
      ]
    }
  },
  "browserslist": [
    "last 2 versions",
    "not dead"
